#include<stdio.h>
#define size 6
int ls(int arr[],int n,int target) 
{
	for(int i=0;i<n;i++)
	{
		if(arr[i]==target) {
			return i;
		}
	}
	return -1;
}
int main()
{
	int arr[size]={23,45,54,32,5,4};
	int target=45;
	int result=ls(arr,size,target);
	if(result != -1){
		printf("element %d found at index %d\n",target,result);
	}else{
		printf("element %d not found in the array\n",target);
	}
	return 0;
}
